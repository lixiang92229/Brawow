<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.widgetcarouselitem.entity.TWidgetCarouselitem" table="widget_carousel_item" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="carousel_id" type="java.lang.Interger">
            <column name="carousel_id" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" length="255" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" length="255" />
        </property>
        <property name="pic_url" type="java.lang.String">
            <column name="pic_url" length="255" />
        </property>
        <property name="caption" type="java.lang.String">
            <column name="caption" length="255" />
        </property>
    </class>
</hibernate-mapping>