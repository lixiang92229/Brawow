<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.userprofile.entity.TUserProfile" table="user_profile" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="user_id" type="java.lang.Interger">
            <column name="user_id" />
        </property>
        <property name="realname" type="java.lang.String">
            <column name="realname" length="255" />
        </property>
        <property name="nickname" type="java.lang.String">
            <column name="nickname" length="255" />
        </property>
        <property name="gender" type="java.lang.Interger">
            <column name="gender" />
        </property>
        <property name="avatar_url" type="java.lang.String">
            <column name="avatar_url" length="255" />
        </property>
        <property name="signature" type="java.lang.String">
            <column name="signature" length="255" />
        </property>
    </class>
</hibernate-mapping>