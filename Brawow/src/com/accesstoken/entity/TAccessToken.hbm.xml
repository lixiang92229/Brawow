<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.accesstoken.entity.TAccessToken" table="access_token" catalog="brawow">
        <id name="id" type="java.lang.Interger">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="access_token" type="java.lang.String">
            <column name="access_token" length="255" />
        </property>
        <property name="end_time" type="java.util.Date">
        	<column name="end_time" />
        </property>
    </class>
</hibernate-mapping>