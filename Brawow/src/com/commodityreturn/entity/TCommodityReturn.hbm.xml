<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.commodityreturn.entity.TCommodityReturn" table="commodity_return" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="order_profile_id" type="java.lang.Integer">
            <column name="order_profile_id" />
        </property>
        <property name="user_id" type="java.lang.Integer">
            <column name="user_id" />
        </property>
        <property name="admin_name" type="java.lang.String">
            <column name="admin_name" length="255" />
        </property>
        <property name="logistics_id_back" type="java.lang.String">
            <column name="logistics_id_back" length="255" />
        </property>
        <property name="logistics_id_return" type="java.lang.String">
            <column name="logistics_id_return" length="255" />
        </property>
        <property name="user_district_id" type="java.lang.Integer">
            <column name="user_district_id" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" length="255" />
        </property>
        <property name="created_time" type="timestamp">
            <column name="created_time" />
        </property>
        <property name="updated_at" type="timestamp">
            <column name="updated_at" />
        </property>
        <property name="state" type="java.lang.Integer">
            <column name="state" />
        </property>
    </class>
</hibernate-mapping>