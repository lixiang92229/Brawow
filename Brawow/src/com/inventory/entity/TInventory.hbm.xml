<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.inventory.entity.TInventory" table="inventory" catalog="brawow">
        <id name="id" type="java.lang.Interger">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="commodity_id" type="java.lang.Interger">
            <column name="commodity_id" />
        </property>
        <property name="size_id" type="java.lang.Interger">
            <column name="size_id" />
        </property>
        <property name="number" type="java.lang.Interger">
            <column name="number" />
        </property>
    </class>
</hibernate-mapping>