<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.user.entity.TUser" table="user" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="username" type="java.lang.String">
            <column name="username" length="32" />
        </property>
        <property name="mobile" type="java.lang.String">
            <column name="mobile" length="16" />
        </property>
        <property name="access_token" type="java.lang.String">
            <column name="access_token" length="32" />
        </property>
        <property name="third_party_id" type="java.lang.String">
            <column name="third_party_id" length="32" />
        </property>
        <property name="password_hash" type="java.lang.String">
            <column name="password_hash" length="255" />
        </property>
        <property name="status" type="java.lang.Interger">
            <column name="status" />
        </property>
        <property name="integral" type="java.lang.Interger">
            <column name="integral" />
        </property>
        
    </class>
</hibernate-mapping>
