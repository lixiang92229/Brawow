<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.shoppingcart.entity.TShoppingCart" table="shopping_cart" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="user_id" type="java.lang.Integer">
            <column name="user_id" />
        </property>
        <property name="commodity_id" type="java.lang.Integer">
            <column name="commodity_id" />
        </property>
        <property name="number" type="java.lang.Integer">
            <column name="number" />
        </property>
        <property name="money" type="java.lang.Float">
            <column name="money" />
        </property>
        <property name="num_money" type="java.lang.Float">
            <column name="mun_money" />
        </property>
    </class>
</hibernate-mapping>