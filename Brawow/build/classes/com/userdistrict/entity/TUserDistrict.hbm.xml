<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.userdistrict.entity.TUserDistrict" table="user_district" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="user_id" type="java.lang.Integer">
            <column name="user_id" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="255" />
        </property>
        <property name="city_id" type="java.lang.Integer">
            <column name="city_id" />
        </property>
        <property name="county_id" type="java.lang.Integer">
            <column name="county_id" />
        </property>
        <property name="mobile_id" type="java.lang.Integer">
            <column name="mobile_id" />
        </property>
        <property name="postcode_id" type="java.lang.Integer">
            <column name="postcode_id" />
        </property>
        <property name="location" type="java.lang.String">
            <column name="location" length="255" />
        </property>
        <property name="text" type="java.lang.String">
            <column name="text" length="255" />
        </property>
    </class>
</hibernate-mapping>