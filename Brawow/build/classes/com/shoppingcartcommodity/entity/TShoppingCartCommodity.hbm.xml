<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.shoppingcartcommodity.entity.TShoppingCartCommodity" table="shoppingcartcommodity" catalog="brawow">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="shopping_cart_id" type="java.lang.Integer">
            <column name="shopping_cart_id" />
        </property>
        <property name="commodity_id" type="java.lang.Integer">
            <column name="commodity_id" />
        </property>
        <property name="number" type="java.lang.Integer">
            <column name="number" />
        </property>
        <property name="money" type="java.lang.Float">
            <column name="money" />
        </property>
    </class>
</hibernate-mapping>